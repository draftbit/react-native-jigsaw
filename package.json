{
  "name": "@draftbit/ui",
  "version": "36.1.4",
  "description": "Jigsaw component library for Draftbit",
  "main": "src/index.js",
  "repository": "git@github.com:draftbit/jigsaw-component-library.git",
  "author": "Draftbit",
  "license": "ISC",
  "private": false,
  "scripts": {
    "babel:build": "babel src -d --out-dir dist --ignore \"**/__tests__\" --copy-files",
    "clean": "del-cli ./dist",
    "lint": "eslint src",
    "test": "jest",
    "seed:db": "node scripts/collector.js",
    "pretty": "prettier --config ./package.json --write '{example,src}/**/*.{js,css,json}'",
    "build:web": "yarn clean && yarn babel:build",
    "sync:glyphmaps": "aws s3 sync glyphmaps s3://static.draftbit.com/fonts/glyphmaps --profile DraftbitS3SharedStatic --exclude \"*\" --include \"*.json\" --acl public-read"
  },
  "jest": {
    "preset": "react-native"
  },
  "peerDependencies": {
    "expo": "^36.0.0",
    "react": "16.9.0",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-react-native": "^4.0.1",
    "del-cli": "^2.0.0",
    "glob": "^7.1.4",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.2",
    "node-fetch": "^2.6.0",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.8.6"
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^3.0.5",
    "@expo/vector-icons": "~10.0.0",
    "color": "^3.1.2",
    "create-react-context": "^0.3.0",
    "dateformat": "^3.0.3",
    "expo-av": "~8.0.0",
    "expo-blur": "~8.0.0",
    "expo-constants": "~8.0.0",
    "react-lifecycles-compat": "^3.0.4",
    "react-native-gesture-handler": "1.5.0",
    "react-native-maps": "0.26.1",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-progress": "^3.6.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-slider": "^0.11.0",
    "react-native-step-indicator": "^0.0.11",
    "react-native-webview": "7.4.3"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": false,
    "semi": false,
    "useTabs": false,
    "printWidth": 100,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "jsxBracketSameLine": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
