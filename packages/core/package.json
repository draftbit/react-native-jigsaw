{
  "name": "@draftbit/core",
  "version": "47.1.0",
  "description": "Core (non-native) Components",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepare": "bob build",
    "clean": "rimraf lib",
    "clean:modules": "rimraf node_modules"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/draftbit/react-native-jigsaw",
    "directory": "packages/core"
  },
  "author": "Draftbit <services@draftbit.com> (https://draftbit.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/draftbit/react-native-jigsaw/issues"
  },
  "homepage": "https://github.com/draftbit/react-native-jigsaw#readme",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@draftbit/react-theme-provider": "^2.1.1",
    "@draftbit/types": "^47.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/pickers": "^3.2.10",
    "@react-native-community/slider": "4.2.4",
    "@react-native-picker/picker": "2.4.8",
    "@shopify/flash-list": "1.3.1",
    "color": "^3.1.2",
    "date-fns": "^2.16.1",
    "dateformat": "^3.0.3",
    "lodash.isnumber": "^3.0.3",
    "lodash.omit": "^4.5.0",
    "lodash.tonumber": "^4.0.3",
    "react-native-deck-swiper": "^2.0.12",
    "react-native-modal-datetime-picker": "^13.0.0",
    "react-native-svg": "13.4.0",
    "react-native-typography": "^1.4.1",
    "react-native-web-swiper": "^2.2.3"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/dateformat": "^3.0.1",
    "@types/lodash.isnumber": "^3.0.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.tonumber": "^4.0.6"
  },
  "react-native-builder-bob": {
    "declarationMap": false,
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "sourceMaps": false
        }
      ],
      [
        "module",
        {
          "sourceMaps": false
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "tsc": "../../node_modules/typescript/bin/tsc"
        }
      ]
    ]
  }
}
