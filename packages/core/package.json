    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.tonumber": "^4.0.6"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "tsc": "../../node_modules/typescript/bin/tsc"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
